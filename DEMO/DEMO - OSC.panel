      <luaMethod luaMethodName="onSlider1Change" luaMethodCode="--&#10;-- Called when a modulator value changes&#10;-- @mod   http://ctrlr.org/api/class_ctrlr_modulator.html&#10;-- @value    new numeric value of the modulator&#10;--&#10;onSlider1Change = function(--[[ CtrlrModulator --]] mod, --[[ number --]] value, --[[ number --]] source)&#13;&#10;&#9;if panel:getLabel(&quot;oscTargetHost&quot;) and panel:getLabel(&quot;oscTargetPort&quot;) then&#13;&#10;&#9;&#9;oscAddress = osc.createAddress(panel:getLabel(&quot;oscTargetHost&quot;):getText(), panel:getLabel(&quot;oscTargetPort&quot;):getText():getIntValue())&#13;&#10;&#9;else&#13;&#10;&#9;&#9;oscAddress = osc.createAddress(&quot;localhost&quot;, &quot;7001&quot;)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;if panel:getLabel(&quot;oscPathLabel&quot;) then&#13;&#10;&#9;&#9;oscMessage = osc.createMessage(panel:getLabel(&quot;oscPathLabel&quot;):getText(), &quot;is&quot;)&#13;&#10;&#9;else&#13;&#10;&#9;&#9;oscMessage = osc.createMessage(&quot;/ctrlr/mod&quot;, &quot;is&quot;)&#13;&#10;&#9;end&#13;&#10;&#9;&#13;&#10;&#9;if panel:getLabel(&quot;textToSend&quot;) then&#13;&#10;&#9;&#9;textToSend = panel:getLabel(&quot;textToSend&quot;):getText();&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;osc.messageAddInt32(oscMessage, value)&#13;&#10;&#9;osc.messageAddString(oscMessage, textToSend)&#13;&#10;&#9;osc.sendMessage(oscAddress, oscMessage)&#10;end"
