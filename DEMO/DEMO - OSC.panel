      <luaMethod luaMethodName="onOSCMessage" luaMethodCode="--&#10;-- Called when a panel receives a OSC message&#10;-- @path - osc path&#10;-- @types - a string representing what types are included in the incoming messages&#10;-- @arguments - a lua table of all incoming arguments&#10;--&#10;&#10;onOSCMessage = function(--[[ string --]] path, --[[ string --]] types, --[[ table --]] arguments)&#13;&#10;&#9;console (&quot;OSC: &quot; .. path .. &quot; types: &quot; .. types)&#13;&#10;&#9;-- console (string.format (&quot;float: %.2f&quot;,arguments[0].f))&#13;&#10;&#13;&#10;&#9;log = panel:getLabel(&quot;oscMonitor&quot;)&#13;&#10;&#9;if log:getText():length() &gt; 300 then&#13;&#10;&#9;&#9;log:setText(&quot;&quot;)&#13;&#10;&#9;end&#13;&#10;&#13;&#10;&#9;log:append(path):append(&quot;, &quot;):append(types):append(&quot;\n&quot;)&#13;&#10;&#9;&#13;&#10;&#13;&#10;&#9;for i=0,#arguments do&#13;&#10;&#9;&#9;type = string.sub(types,i+1,i+1) &#9;-- in lua we can't use [] for strings&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- +1 index since arguments is a 0 indexed array&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- and lua starts with 1&#13;&#10;&#13;&#10;&#9;&#9;if type == 's' then&#13;&#10;&#9;&#9;&#9;what(arguments[i])&#13;&#10;&#9;&#9;&#9;log:append(string.format(&quot;[string: %s] &quot;, arguments[i]))-- we can't use .s since it points&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- to a single character, we just&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- use the entire message here, with .s&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;-- we'll get nil&#13;&#10;&#13;&#10;&#9;&#9;elseif type == 'i'  then&#13;&#10;&#13;&#10;&#9;&#9;&#9;log:append(string.format(&quot;[int: %d] &quot;, arguments[i].i))&#13;&#10;&#13;&#10;&#9;&#9;elseif type == 'f' then&#13;&#10;&#13;&#10;&#9;&#9;&#9;log:append(string.format(&quot;[float: %.2f] &quot;, arguments[i].f))&#13;&#10;&#13;&#10;&#9;&#9;end&#13;&#10;&#13;&#10;&#9;end&#13;&#10;&#9;log:append(&quot;\n&quot;)&#10;end"
